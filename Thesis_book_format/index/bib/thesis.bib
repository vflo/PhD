issn = {0829-318X},
journal = {Tree Physiology},
keywords = {a practical consequence of,and low sap flow,at night in all,ation,difficulty,greenspan technology sensors,measurement vari-,nocturnal,of distinguishing between zero,sap velocity,thermal balance,thermistor,these limitations is the},
number = {3},
pages = {177--184},
pmid = {12651387},
title = {{Limitations of a compensation heat pulse velocity system at low sap flow: implications for measurements at night and in shaded trees}},
