journal = {Epilepsy {\&} behavior : E{\&}B},
keywords = {Adult,Disease Management,Early Medical Intervention,Early Medical Intervention: methods,Epilepsy,Epilepsy: diagnosis,Epilepsy: psychology,Epilepsy: therapy,Humans,Quality of Life,Quality of Life: psychology,Randomized Controlled Trials as Topic,Randomized Controlled Trials as Topic: methods,Self Care,Self Care: methods,Self Care: psychology},
month = {apr},
