issn = {0342-7188},
journal = {Irrigation Science},
number = {1},
pages = {1--8},
title = {{Determination of transpiration in irrigated grapevines: comparison of the heat-pulse technique with gravimetric and micrometeorological methods}},
volume = {20},
year = {2000}
}
