journal = {Experimental dermatology},
keywords = {Animals,DNA Repair,Free Radical Scavengers,Free Radical Scavengers: metabolism,Gene Expression,Hair Follicle,Hair Follicle: metabolism,Humans,Melatonin,Melatonin: metabolism,Melatonin: radiation effects,Radiation, Ionizing,Receptors, Melatonin,Receptors, Melatonin: metabolism,Skin,Skin: metabolism,Skin: radiation effects,Vitiligo,Vitiligo: metabolism},
