@article{Miralles2011,
abstract = {A process-based methodology is applied to estimate land-surface$\backslash$nevaporation from multi-satellite information. GLEAM (Global Land-surface$\backslash$nEvaporation: the Amsterdam Methodology) combines a wide range of$\backslash$nremotely-sensed observations to derive daily actual evaporation and its$\backslash$ndifferent components. Soil water stress conditions are defined from a$\backslash$nroot-zone profile of soil moisture and used to estimate transpiration$\backslash$nbased on a Priestley and Taylor equation. The methodology also derives$\backslash$nevaporationfrom bare soil and snow sublimation. Tall vegetation rainfall$\backslash$ninterception is independently estimated by means of the Gash analytical$\backslash$nmodel. Here, GLEAM is applied daily, at global scale and a quarter$\backslash$ndegree resolution. Triple collocation is used to calculate the error$\backslash$nstructure of the evaporation estimates and test the relative merits of$\backslash$ntwo different precipitation inputs. The spatial distribution of$\backslash$nevaporation - and its different components - is analysed to understand$\backslash$nthe relative importance of each component over different ecosystems.$\backslash$nAnnual land evaporation is estimated as 67.9 x 10(3) km(3), 80{\%}$\backslash$ncorresponding to transpiration, 11{\%} to interception loss, 7{\%} to bare$\backslash$nsoil evaporation and 2{\%} snow sublimation. Results show that rainfall$\backslash$ninterception plays an important role in the partition of precipitation$\backslash$ninto evaporation and water available for runoff at a continental scale.$\backslash$nThis study gives insights into the relative importance of precipitation$\backslash$nand net radiation in driving evaporation, and how the seasonal influence$\backslash$nof these controls varies over different regions. Precipitation is$\backslash$nrecognised as an important factor driving evaporation, not only in areas$\backslash$nthat have limited soil water availability, but also in areas of high$\backslash$nrainfall interception and low available energy.},
author = {Miralles, D. G. and {De Jeu}, R. A.M. and Gash, J. H. and Holmes, T. R.H. and Dolman, A. J.},
doi = {10.5194/hess-15-967-2011},
