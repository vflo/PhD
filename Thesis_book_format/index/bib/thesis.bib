isbn = {0829-318X},
journal = {Tree Physiology},
number = {2},
pages = {179--190},
title = {{the Effect of Environmentally-Induced Stem Temperature-Gradients on Transpiration Estimates From the Heat-Balance Method in 2 Tropical Woody Species}},
volume = {14},
