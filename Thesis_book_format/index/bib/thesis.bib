file = {:F$\backslash$:/Users/v.flo/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Sakuratani - 1981 - A Heat Balance Method for Measuring Water Flux in the Stem of Intact Plants.pdf:pdf},
