@article{Rosas-Guerrero2014,
abstract = {The idea of pollination syndromes has been largely discussed but no formal quantitative evaluation has yet been conducted across angiosperms. We present the first systematic review of pollination syndromes that quantitatively tests whether the most effective pollinators for a species can be inferred from suites of floral traits for 417 plant species. Our results support the syndrome concept, indicating that convergent floral evolution is driven by adaptation to the most effective pollinator group. The predictability of pollination syndromes is greater in pollinator-dependent species and in plants from tropical regions. Many plant species also have secondary pollinators that generally correspond to the ancestral pollinators documented in evolutionary studies. We discuss the utility and limitations of pollination syndromes and the role of secondary pollinators to understand floral ecology and evolution.},
author = {Rosas-Guerrero, V{\'{i}}ctor and Aguilar, Ramiro and Mart{\'{e}}n-Rodr{\'{i}}guez, Silvana and Ashworth, Lorena and Lopezaraiza-Mikel, Martha and Bastida, Jes{\'{u}}s M and Quesada, Mauricio},
doi = {10.1111/ele.12224},
issn = {1461-0248},
journal = {Ecology letters},
keywords = {17,2014,388,400,ecology letters,floral evolution,floral syndromes,lination efficiency,meta-analysis,plant breeding systems,plant reproduction,pol-,pollination networks,specialisation,tropical ecology},
month = {mar},
number = {3},
pages = {388--400},
pmid = {24393294},
title = {{A quantitative review of pollination syndromes: do floral traits predict effective pollinators?}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/24393294},
volume = {17},
year = {2014}
}
@article{Sol2012,
abstract = {Despite considerable current interest in biological invasions, the common life-history characteristics of successful invaders remain elusive. The widely held hypothesis that successful invaders have high reproductive rates has received little empirical support; however, alternative possibilities are seldom considered. Combining a global comparative analysis of avian introductions ({\textgreater}2700 events) with demographic models and phylogenetic comparative methods, we show that although rapid population growth may be advantageous during invasions under certain circumstances, more generally successful invaders are characterized by life-history strategies in which they give priority to future rather than current reproduction. High future breeding expectations reduce the costs of reproductive failure under uncertain conditions and increase opportunities to explore the environment and respond to novel ecological pressures.},
