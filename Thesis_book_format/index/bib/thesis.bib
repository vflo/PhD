author = {Martinez-Vilalta, J and Cochard, Herv{\'{e}} and Mencuccini, M and Sterck, F J and Herrero, A and Korhonen, J F K and Llorens, P and Nikinmaa, E and Nol{\`{e}}, A and Poyatos, R and Ripullone, F and Sass-Klaassen, U and Zweifel, R},
