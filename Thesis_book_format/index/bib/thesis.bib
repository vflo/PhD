issn = {1474-905X},
journal = {Photochemical {\&} photobiological sciences : Official journal of the European Photochemistry Association and the European Society for Photobiology},
keywords = {Animals,Antineoplastic Agents, Phytogenic,Antineoplastic Agents, Phytogenic: therapeutic use,Antioxidants,Antioxidants: therapeutic use,Chemoprevention,Dietary Supplements,Humans,Neoplasms, Radiation-Induced,Neoplasms, Radiation-Induced: prevention {\&} control,Phytotherapy,Skin,Skin Neoplasms,Skin Neoplasms: drug therapy,Skin Neoplasms: prevention {\&} control,Skin: chemistry,Skin: radiation effects,Ultraviolet Rays,Ultraviolet Rays: adverse effects},
