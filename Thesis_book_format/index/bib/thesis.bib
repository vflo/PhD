journal = {Ecology Letters},
keywords = {Animals,Biological,Ecosystem,Insects,Insects: physiology,Models,Plant Physiological Phenomena,Pollen,Pollen: cytology,Pollination,Pollination: physiology,Spain,Species Specificity},
month = {may},
number = {5},
