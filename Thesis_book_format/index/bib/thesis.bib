doi = {10.1093/treephys/tpv013},
file = {:F$\backslash$:/Users/v.flo/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Niu et al. - 2015 - Oil palm water use Calibration of a sap flux method and a field measurement scheme.pdf:pdf},
isbn = {0829318X},
issn = {17584469},
journal = {Tree Physiology},
keywords = {environmental drivers,error margins,sample size,thermal dissipation probes,transpiration},
number = {5},
pages = {563--573},
