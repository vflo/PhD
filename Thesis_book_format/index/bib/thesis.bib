pages = {1467--1475},
publisher = {Elsevier B.V.},
title = {{Sap flux-scaled transpiration by tamarisk (Tamarix spp.) before, during and after episodic defoliation by the saltcedar leaf beetle (Diorhabda carinulata)}},
