keywords = {Humans,Sunburn,Sunburn: prevention {\&} control,beta Carotene,beta Carotene: administration {\&} dosage,beta Carotene: therapeutic use},
number = {2},
pages = {284--8},
pmid = {18086246},
title = {{Protection from sunburn with beta-Carotene--a meta-analysis.}},
