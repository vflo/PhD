@article{Smith1996a,
abstract = {Transpiration rates for whole plants, individual branches or tillers can be determined by techniques which measure the rate at which sap ascends stems. All of these methods use heat as a tracer for sap movement, but they are fundamentally different in their operating principles. Two methods commonly employed, the stem heat balance and trunk sector heat balance methods, use the heat balance principle; the stem is heated electrically and the heat balance is solved for the amount of heat taken up by the moving sap stream, which is then used to calculate the mass flow of sap in the stem. In the heat-pulse method, rather than using continuous heating, short pulses of heat are applied and the mass flow of sap is deter- mined from the velocity of the heat pulses moving along the stem. In addition, rates of sap flow can be determined empirically, using the thermal dissipation technique, from the temperature of sapwood near a continuously-powered heater implanted in the stem. Users must understand the theory underlying each of these methods, so that they can select the method most appropriate to their application and take pre- cautions against potential sources of error. When attempting to estimate transpiration by stands of vegetation from measurements of sap flow in individual plants, users must also select an appropriate sampling strategy and scaling method. Key},
author = {Smith, D.M. and Allen, S.J.},
doi = {10.1093/jxb/47.12.1833},
file = {:F$\backslash$:/Users/v.flo/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Smith, Allen - 1996 - Measurement of sap flow in plant stems(2).pdf:pdf},
isbn = {0022-0957},
issn = {0022-0957},
journal = {Journal of Experimental Botany},
keywords = {heat pulse velocity,review,sap flow,stem heat balance,transpiration},
number = {12},
pages = {1833--1844},
title = {{Measurement of sap flow in plant stems}},
url = {https://academic.oup.com/jxb/article-lookup/doi/10.1093/jxb/47.12.1833},
volume = {47},
year = {1996}
}
