pages = {7978--85},
pmid = {23630271},
title = {{Used planet: a global history.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3657770{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {110},
year = {2013}
}
