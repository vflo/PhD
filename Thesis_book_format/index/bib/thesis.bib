journal = {Nature communications},
keywords = {Ecosystem,Lakes,Nitrogen,Nitrogen: analysis,Nitrogen: metabolism,Phosphorus,Phosphorus: analysis,Phosphorus: metabolism,Phytoplankton,Phytoplankton: metabolism},
month = {jan},
