file = {:F$\backslash$:/Users/v.flo/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Clearwater et al. - 2009 - An external heat pulse method for measurement of sap flow through fruit pedicels, leaf petioles and other sma.pdf:pdf},
isbn = {1365-3040},
issn = {01407791},
journal = {Plant, Cell and Environment},
keywords = {Actinidia chinensis,Actinidia deliciosa,Fruit,Heat pulse velocity,Phloem,Transpiration,Xylem},
number = {12},
pages = {1652--1663},
