doi = {10.1212/WNL.0000000000001728},
file = {:F$\backslash$:/Users/v.flo/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Hixson et al. - 2015 - Patients optimizing epilepsy management via an online community the POEM Study.pdf:pdf},
issn = {1526-632X},
journal = {Neurology},
keywords = {Adult,Community Networks,Epilepsy,Epilepsy: psychology,Epilepsy: therapy,Female,Humans,Internet,Male,Middle Aged,Online Systems,Residence Characteristics,Self Care,Self Care: methods,Self Efficacy,Social Support,Veterans},
month = {jul},
